{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Collection of notes",
  "properties": {
    "notes": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "note.json"
      }
    },
    "total_records": {
      "type": "integer"
    }
  },
  "required": [
    "notes",
    "total_records"
  ]
}
